    "homepage": "https://github.com/sedehi/laravel-tools",
    "keywords": ["Laravel", "LaravelTools"],
    "require": {
        "illuminate/support": ">=6",
        "hekmatinasser/verta": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "orchestra/testbench": ">=3"
    },
    "autoload": {
        "psr-4": {
            "Sedehi\\LaravelTools\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sedehi\\LaravelTools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sedehi\\LaravelTools\\LaravelToolsServiceProvider"
            ]
        }
    }
}
